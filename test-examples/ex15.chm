struct LinkedList {
    data: int
    next: LinkedList
}

fun main(): int [
    list = alloc LinkedList {
        data = 65
        next = alloc LinkedList {
            data = 66
            next = alloc LinkedList {
                data = 67
                next = alloc LinkedList {
                    data = 68
                    next = null
                }
            }
        }
    }
    current = list
    while current != null [
        putchar(current.data)
        current = current.next
    ]
    putchar(10)
    return 10
]